		.data
matrice: .word 1, 41, 42, 13, 56, 23, 73, 9, 50
mex:	.asciiz "Valore determinante: "
		.text
		.globl main
		.ent main
main:	addi $sp, $sp, -4
		sw $ra, ($sp)
		la $t0, matrice
		lw $a0, ($t0)
		lw $a1, 4($t0)
		lw $a2, 8($t0)
		lw $a3, 12($t0)
		move $t1, $0	#indice ciclo
ciclo:	lw $t2, 16($t0)
		addi $sp, $sp, -4
		sw $t2, ($sp)
		addi $t0, $t0, 4
		addi $t1, $t1, 1
		bne $t1, 5, ciclo
		jal determinante3x3
		move $t0, $v0
		li $v0, 4
		la $a0, mex
		syscall
		move $a0, $t0
		li $v0, 1
		syscall
		lw $ra, 20($sp)
		addi $sp, $sp, 24
		jr $ra
		.end main
		
determinante3x3:
		addi $fp, $fp, -4 # ???????????????????????????????????????????????????????
		addi $sp, $sp, -20
		sw $ra, ($sp)
		sw $s0, 4($sp)
		sw $s1, 8($sp)
		sw $s2, 12($sp)
		sw $s3, 16($sp)
		move $s0, $a0	#a1
		move $s1, $a1	#b1
		move $s2, $a2	#c1
		move $s3, $a3	#a2
		
		lw $a0, 20($fp) #b2
		lw $a1, 16($fp) #c2
		lw $a2, 8($fp) #b3
		lw $a3, 4($fp) #c3
		jal determinante2x2
		mul $s0, $s0, $v0
		
		move $a0, $s3
		lw $a1, 16($fp) 
		lw $a2, 12($fp) #a3
		lw $a3, 4($fp)
		jal determinante2x2
		mul $s1, $s1, $v0
		
		move $a0, $s3
		lw $a1, 20($fp)
		lw $a2, 12($fp)
		lw $a3, 8($fp)
		jal determinante2x2
		mul $s2, $s2, $v0
		
		sub $v0, $s0, $s1
		add $v0, $v0, $s2
		lw $s0, 4($sp)
		lw $s1, 8($sp)
		lw $s2, 12($sp)
		lw $s3, 16($sp)
		lw $ra, ($sp)
		addi $sp, $sp, 20
		jr $ra
		
determinante2x2:
		mul $a0, $a0, $a3
		mul $a2, $a2, $a1
		sub $v0, $a0, $a2
		jr $ra